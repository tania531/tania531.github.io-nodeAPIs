


var distance = 0;


var p1 = {x: 3, y: 7};
var p2 = {x: 8, y: 15};
// var d = distance(p1, p2);
// d === ???? some number

var dx = p2.x - p1.x;

var dy = p2.y - p1.y;

function getDistance(p1, p2){
  x = p2.x - p1.x;
  y = p2.y - p1.y;

  distance =Math.sqrt(Math.pow(x, 2) + Math.pow(y,2));
  return distance;
}


var result = getDistance(p1, p2);

console.log('result: ', result);


